public class OSClock
{
    private static long time = 0;
    
    public static long getClock()
    {
        return time;
    }
    
    public static void setClock(int x)
    {
        time = x;
    }
    
    public static void incrementClock()
    {
        time++;
    }
    
    public static void resetClock()
    {
        time = 0;
    }
    
}
